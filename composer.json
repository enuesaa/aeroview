{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "enuesaa/aeroview",
    "type": "project",
    "description": "",
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "guzzlehttp/guzzle": "^7.9",
        "laravel/framework": "^11.31",
        "laravel/octane": "^2.8",
        "zircote/swagger-php": "^5.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.5",
        "brianium/paratest": "^7.8",
        "composer/composer": "^2.8",
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.13",
        "league/openapi-psr7-validator": "^0.22.0",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "nyholm/psr7": "^1.8",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": "App\\Support\\ComposerScripts::postInstall",
        "post-update-cmd": "./artisan vendor:publish --tag=laravel-assets --ansi --force",
        "buildui": "npm install && npm run build",
        "format": "pint --preset psr12",
        "stan": "phpstan analyse",
        "tailog": "./artisan pail",
        "clear": "./artisan optimize:clear",
        "test": "./artisan migrate --env=testing && ./artisan test",
        "modeldocgen": "./artisan ide-helper:models --write --no-interaction"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
